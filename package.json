{
  "name": "tddforjs",
  "version": "4.0.0",
  "description": "A Test Driven Development framework for Javascript.",
  "preferGlobal": false,
  "main": "./TDDforJS.js",
  "author": {
    "name": "Joseph Spencer",
    "email": "js.developer.undefined@gmail.com"
  },
  "bin":{
    "tddforjs":"build/TDDforJS.js"
  },
  "dependencies":[
    "config-tools"
  ],
  "devDependencies":{
    "xforjs":"x.x.x"
  },
  "scripts":{
    "prepublish":"npm run-script build",
